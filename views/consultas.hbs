<div class="container text-center  ">

    <h3 >
        Listado de Productos
    </h3>

</div>


<div class="table-responsive">
<table class="table table-dark table-striped">
    <thead>
        <th>Id Productos</th>
        <th>Nombre</th>
        <th>Marca</th>
        <th>Precio</th>
        <th>Accion</th>
    </thead>
    <tbody>
        {{#each result}}
        <tr>
            <td>{{_id}}</td>
            <td>{{nombre}}</td>
            <td>{{marca}}</td>
            <td>{{precio}}</td>
            <td>
                <a href="eliminar/{{_id}}" class="btn btn-danger">Eliminar</a>
                <a href="actualizar/{{_id}}" class="btn btn-warning">Actualizar</a>
            </td>
        </tr>
    </tbody>
    {{/each}}

</table>


</div>
